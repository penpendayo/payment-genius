{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/usage.tsx", "component/utils/soundEffect.ts", "pages/_document.tsx", "component/app/App.tsx", "pages/edu.tsx", "component/styles/Money.tsx", "component/utils/mediaQuery.ts"],
  "exclude": ["node_modules"]
}
